---
layout: nil
---

[
{%- for post in site.posts -%}
    {
      "title" : "{{ post.title }}",
      "date" : "{{ post.date }}",
      "link" : "{{ post.url }}",
      "media" : "<img class='card-img-top' src='{%- if post.image -%}blog.whsrobotics.org{{ post.image }}{%- else -%}favicon.ico{%- endif -%}'>",
      "content" : "{{ post.excerpt | escape }}"
    } {%- if forloop.last -%}
      {%- else -%}
        ,
      {%- endif -%}
{%- endfor -%}
]